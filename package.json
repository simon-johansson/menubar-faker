{
  "name": "menubar-faker",
  "productName": "MenubarFaker",
  "version": "0.0.0",
  "description": "My divine app",
  "license": "MIT",
  "repository": "simon-johansson/menubar-faker",
  "author": {
    "name": "Simon Johansson",
    "email": "",
    "url": "example.com"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "xo",
    "start": "babel electron.js --out-file index.js && electron .",
    "dev": "browserify app.js -t babelify --outfile bundle.js",
    "watch": "watchify app.js --watch -t babelify --outfile bundle.js",
    "build": "electron-packager . $npm_package_productName --out=dist --ignore='^/dist$' --prune --asar --all --version=0.31.1"
  },
  "files": [
    "index.js",
    "index.html",
    "index.css"
  ],
  "keywords": [
    "electron-app"
  ],
  "dependencies": {
    "applescript": "^1.0.0",
    "bootstrap": "^3.3.5",
    "electron-debug": "^0.1.1",
    "faker": "^3.0.1",
    "jquery": "^2.1.4",
    "menubar": "^2.2.1"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babelify": "^6.3.0",
    "browserify": "^11.1.0",
    "electron-packager": "^5.0.0",
    "electron-prebuilt": "^0.31.1",
    "watchify": "^3.4.0",
    "xo": "^0.7.1"
  },
  "xo": {
    "esnext": true,
    "envs": [
      "node",
      "browser"
    ],
    "rules": {
      "prefer-arrow-callback": 0
    }
  }
}
